  
{
    "manifest_version": 2,
  
    "name": "Note taker",
    "description": "Note taking at instance ",
    "version": "1.0.0",
    "content_scripts" : [
      {   
          "matches" : [
              "<all_urls>"
          ],
          "js": ["content.js"]
     }
  ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
  }